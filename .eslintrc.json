{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-extra-semi": "warn",
    "no-undef": "warn",
    "linebreak-style": ["error", "windows"],
    "no-unused-vars": "warn",
    "require-jsdoc": "off"
  },
  "globals": {
    "alert": false,
    "document": false,
    "console": false,
    "chrome": false
  }
}
