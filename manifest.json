{
	"manifest_version": 2,
	"author": "nitech Create",
	"homepage_url": "http://nitech-create.com",
	"name": "moodle assistant for Nitech",
	"description": "名古屋工業大学のmoodleを使いやすくします。",
	"version": "0.0.0.1",
	"icons": {
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"content_scripts": [{
        "matches": ["https://cms6.ict.nitech.ac.jp/moodle38a/*"],
        "css":["styles.css"],
		"js": ["jquery.min.js","script.js"]
	}],
	"background": {
		"scripts": []
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": ""
	},
	"permissions": [
		"tabs",
		"background",
		"http://*/*",
		"https://*/*",
		"storage"
	]
}